																									<xsl:variable name="quoteNetPrice">
																										<xsl:choose>
																											<xsl:when test="$_dsMain1/pricingTierForPrint_t_c = '1' or $_dsMain1/pricingTierForPrint_t_c = ''">
																												<xsl:value-of select="sum($StorageLines/extendedNetPrice_l_c)"/>
																											</xsl:when>
