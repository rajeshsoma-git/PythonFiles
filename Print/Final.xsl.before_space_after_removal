																													<xsl:when test="string(number(./extendedQuantity_l_c))='NaN' or not(contains(./extendedQuantity_l_c, '.') or contains(./extendedQuantity_l_c, ','))">
																														<xsl:value-of select="./extendedQuantity_l_c"/>
																													</xsl:when>
																													<xsl:otherwise>
																														<xsl:call-template name="BMI_formatFloat">
																															<xsl:with-param name="val" select="./extendedQuantity_l_c"/>
																															<xsl:with-param name="numFormat" select="$EMAIL_RECIPIENT_NUMFORMAT_PREF"/>
