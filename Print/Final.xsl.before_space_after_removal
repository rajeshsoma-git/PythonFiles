<?xml version='1.0' encoding='UTF-8'?>
<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xhtml="http://www.w3.org/1999/xhtml" xmlns:HeadingUtil="com.bm.xchange.services.templateengine.headingstyle.TeHeadingStyleUtil" xmlns:axf="http://www.antennahouse.com/names/XSL/Extensions" xmlns:PrintUtil="com.bm.xchange.services.templateengine.utils.TePrintUtil" xmlns:xfc="http://www.xmlmind.com/foconverter/xsl/extensions" xmlns:FunctionUtil="com.bm.xchange.services.templateengine.utils.TeFunctionUtil" xmlns:exsl="http://exslt.org/common" xmlns="http://www.w3.org/1999/xhtml" version="1.0" extension-element-prefixes="exsl">
	<!-- Parameters -->
	<xsl:param name="EMAIL_RECIPIENT_NUMFORMAT_PREF" select="''"/>
	<xsl:param name="BM_IMAGESERVER_TOKEN" select="''"/>
	<xsl:param name="TIME_ZONE" select="''"/>
	<xsl:param name="TEMPLATE_ID" select="''"/>
	<xsl:param name="TRANSACTION_ID" select="''"/>
	<xsl:param name="PRINT_TIME" select="''"/>
	<xsl:param name="PRINT_CODE" select="''"/>
	<!-- Output method -->
	<xsl:output method="html" encoding="UTF-8" indent="yes"/>
	<!-- convertJsonToXml template for standalone mode -->
	<xsl:template name="convertJsonToXml">
		<xsl:param name="jsonStr"/>
		<object>
			<xsl:call-template name="parseJsonArray">
				<xsl:with-param name="jsonStr" select="$jsonStr"/>
			</xsl:call-template>
		</object>
	</xsl:template>
	<!-- Helper template to parse JSON array -->
	<xsl:template name="parseJsonArray">
		<xsl:param name="jsonStr"/>
		<xsl:variable name="cleanJson" select="normalize-space($jsonStr)"/>
		<!-- Remove outer brackets [ ] -->
		<xsl:variable name="innerJson" select="substring($cleanJson, 2, string-length($cleanJson) - 2)"/>
		<!-- Parse all objects in the array -->
		<xsl:call-template name="parseMultipleJsonObjects">
			<xsl:with-param name="jsonStr" select="$innerJson"/>
		</xsl:call-template>
	</xsl:template>
	<!-- Helper template to parse multiple JSON objects -->
	<xsl:template name="parseMultipleJsonObjects">
		<xsl:param name="jsonStr"/>
		<xsl:variable name="cleanJson" select="normalize-space($jsonStr)"/>
		<xsl:if test="contains($cleanJson, '{')">
			<!-- Extract first object -->
			<xsl:variable name="afterOpen" select="substring-after($cleanJson, '{')"/>
			<xsl:variable name="objContent" select="substring-before($afterOpen, '}')"/>
			<object>
				<xsl:call-template name="parseJsonObject">
					<xsl:with-param name="jsonStr" select="$objContent"/>
				</xsl:call-template>
			</object>
			<!-- Process remaining objects -->
			<xsl:variable name="remaining" select="substring-after($afterOpen, '}')"/>
			<xsl:if test="contains($remaining, '{')">
				<xsl:call-template name="parseMultipleJsonObjects">
					<xsl:with-param name="jsonStr" select="$remaining"/>
				</xsl:call-template>
			</xsl:if>
		</xsl:if>
	</xsl:template>
	<!-- Helper template to parse JSON object -->
	<xsl:template name="parseJsonObject">
		<xsl:param name="jsonStr"/>
		<xsl:variable name="cleanJson" select="normalize-space($jsonStr)"/>
		<xsl:if test="string-length($cleanJson) &gt; 0">
			<xsl:choose>
				<xsl:when test="contains($cleanJson, ',')">
					<!-- Process first property -->
					<xsl:variable name="firstProp" select="normalize-space(substring-before($cleanJson, ','))"/>
					<xsl:call-template name="parseJsonProperty">
						<xsl:with-param name="propStr" select="$firstProp"/>
					</xsl:call-template>
					<!-- Process remaining properties -->
					<xsl:call-template name="parseJsonObject">
						<xsl:with-param name="jsonStr" select="substring-after($cleanJson, ',')"/>
					</xsl:call-template>
				</xsl:when>
				<xsl:otherwise>
					<!-- Process last property -->
					<xsl:call-template name="parseJsonProperty">
						<xsl:with-param name="propStr" select="$cleanJson"/>
					</xsl:call-template>
				</xsl:otherwise>
			</xsl:choose>
		</xsl:if>
	</xsl:template>
	<!-- Helper template to parse individual JSON property -->
	<xsl:template name="parseJsonProperty">
		<xsl:param name="propStr"/>
		<xsl:variable name="cleanProp" select="normalize-space($propStr)"/>
		<xsl:if test="contains($cleanProp, ':')">
			<xsl:variable name="key" select="normalize-space(substring-before($cleanProp, ':'))"/>
			<xsl:variable name="value" select="normalize-space(substring-after($cleanProp, ':'))"/>
			<!-- Remove quotes from key -->
			<xsl:variable name="cleanKey">
				<xsl:choose>
					<xsl:when test="starts-with($key, '&quot;') and substring($key, string-length($key)) = '&quot;'">
						<xsl:value-of select="substring($key, 2, string-length($key) - 2)"/>
					</xsl:when>
					<xsl:otherwise>
						<xsl:value-of select="$key"/>
					</xsl:otherwise>
				</xsl:choose>
			</xsl:variable>
			<!-- Remove quotes from value -->
			<xsl:variable name="cleanValue">
				<xsl:choose>
					<xsl:when test="starts-with($value, '&quot;') and substring($value, string-length($value)) = '&quot;'">
						<xsl:value-of select="substring($value, 2, string-length($value) - 2)"/>
					</xsl:when>
					<xsl:otherwise>
						<xsl:value-of select="$value"/>
					</xsl:otherwise>
				</xsl:choose>
			</xsl:variable>
			<object name="{$cleanKey}">
				<xsl:value-of select="$cleanValue"/>
			</object>
		</xsl:if>
	</xsl:template>
	<!-- Reusable template for formatting account addresses from JSON -->
	<!-- Performance optimized: direct field extraction without full JSON parsing -->
	<xsl:template name="formatAccountAddress">
		<xsl:param name="accountType"/>
		<xsl:variable name="jsonStr" select="string(/transaction/data_xml/document/accountsJsonArray_t_c)"/>
		<!-- Find the account object for the specified type -->
		<xsl:variable name="typePattern" select="concat('&quot;type&quot;:&quot;', $accountType, '&quot;')"/>
		<xsl:if test="contains($jsonStr, $typePattern)">
			<!-- Extract the object containing this type -->
			<xsl:variable name="afterType" select="substring-after($jsonStr, $typePattern)"/>
			<xsl:variable name="objectStr" select="substring-before($afterType, '},')"/>
			<!-- Extract name field -->
			<xsl:variable name="nameValue">
				<xsl:if test="contains($objectStr, '&quot;name&quot;:&quot;')">
					<xsl:value-of select="substring-before(substring-after($objectStr, '&quot;name&quot;:&quot;'), '&quot;')"/>
				</xsl:if>
			</xsl:variable>
			<!-- Extract address1/street field -->
			<xsl:variable name="streetValue">
				<xsl:choose>
					<xsl:when test="contains($objectStr, '&quot;address1&quot;:&quot;')">
						<xsl:value-of select="substring-before(substring-after($objectStr, '&quot;address1&quot;:&quot;'), '&quot;')"/>
					</xsl:when>
					<xsl:when test="contains($objectStr, '&quot;street&quot;:&quot;')">
						<xsl:value-of select="substring-before(substring-after($objectStr, '&quot;street&quot;:&quot;'), '&quot;')"/>
					</xsl:when>
				</xsl:choose>
			</xsl:variable>
			<!-- Extract city field -->
			<xsl:variable name="cityValue">
				<xsl:if test="contains($objectStr, '&quot;city&quot;:&quot;')">
					<xsl:value-of select="substring-before(substring-after($objectStr, '&quot;city&quot;:&quot;'), '&quot;')"/>
				</xsl:if>
			</xsl:variable>
			<!-- Extract state field -->
			<xsl:variable name="stateValue">
				<xsl:if test="contains($objectStr, '&quot;state&quot;:&quot;')">
					<xsl:value-of select="substring-before(substring-after($objectStr, '&quot;state&quot;:&quot;'), '&quot;')"/>
				</xsl:if>
			</xsl:variable>
			<!-- Extract country field -->
			<xsl:variable name="countryValue">
				<xsl:if test="contains($objectStr, '&quot;country&quot;:&quot;')">
					<xsl:value-of select="substring-before(substring-after($objectStr, '&quot;country&quot;:&quot;'), '&quot;')"/>
				</xsl:if>
			</xsl:variable>
			<!-- Extract zipCode field -->
			<xsl:variable name="zipValue">
				<xsl:if test="contains($objectStr, '&quot;zipCode&quot;:&quot;')">
					<xsl:value-of select="substring-before(substring-after($objectStr, '&quot;zipCode&quot;:&quot;'), '&quot;')"/>
				</xsl:if>
			</xsl:variable>
			<!-- Format the address: Name, Street City State Country ZipCode -->
			<xsl:if test="$nameValue != ''">
				<xsl:value-of select="$nameValue"/>
				<xsl:if test="$streetValue != '' or $cityValue != '' or $stateValue != '' or $countryValue != '' or $zipValue != ''">
					<xsl:text>, </xsl:text>
				</xsl:if>
			</xsl:if>
			<xsl:if test="$streetValue != ''">
				<xsl:value-of select="$streetValue"/>
				<xsl:if test="$cityValue != '' or $stateValue != '' or $countryValue != '' or $zipValue != ''">
					<xsl:text> </xsl:text>
				</xsl:if>
			</xsl:if>
			<xsl:if test="$cityValue != ''">
				<xsl:value-of select="$cityValue"/>
				<xsl:if test="$stateValue != '' or $countryValue != '' or $zipValue != ''">
					<xsl:text> </xsl:text>
				</xsl:if>
			</xsl:if>
			<xsl:if test="$stateValue != ''">
				<xsl:value-of select="$stateValue"/>
				<xsl:if test="$countryValue != '' or $zipValue != ''">
					<xsl:text> </xsl:text>
				</xsl:if>
			</xsl:if>
			<xsl:if test="$countryValue != ''">
				<xsl:value-of select="$countryValue"/>
				<xsl:if test="$zipValue != ''">
					<xsl:text> </xsl:text>
				</xsl:if>
			</xsl:if>
			<xsl:if test="$zipValue != ''">
				<xsl:value-of select="$zipValue"/>
			</xsl:if>
		</xsl:if>
	</xsl:template>
	<!-- Variables -->
	<xsl:variable name="FILEATTACHMENT_DELIM" select="'|^|'"/>
	<!-- Template to copy all nodes, given a root node. E.g. Used to copy RTE attribute as it is, in email templates, from the input XML  -->
	<xsl:template name="copyNodes">
		<xsl:param name="rootNode"/>
		<xsl:copy-of select="$rootNode/*"/>
	</xsl:template>
	<xsl:template name="BMI_universalFormatPriceCustom">
		<!-- Simplified wrapper - delegates to formatPrice template -->
		<xsl:param name="price"/>
		<xsl:param name="multiplier" select="1"/>
		<xsl:param name="showCents" select="true()"/>
		<xsl:param name="precision" select="2"/>
		<xsl:param name="currency" select="'USD'"/>
		<xsl:param name="showCurrencySymbol"/>
		<xsl:param name="decimalFormatClass"/>
		<!-- Calculate amount with multiplier -->
		<xsl:variable name="finalAmount" select="number($price) * number($multiplier)"/>
		<!-- Determine final precision -->
		<xsl:variable name="finalPrecision">
			<xsl:choose>
				<xsl:when test="not($showCents)">0</xsl:when>
				<xsl:when test="number($precision) &gt;= 0 and number($precision) &lt;= 8">
					<xsl:value-of select="$precision"/>
				</xsl:when>
				<xsl:otherwise>2</xsl:otherwise>
			</xsl:choose>
		</xsl:variable>
		<!-- Use simplified formatPrice template -->
		<xsl:call-template name="formatPrice">
			<xsl:with-param name="amount" select="$finalAmount"/>
			<xsl:with-param name="currencyCode" select="$currency"/>
			<xsl:with-param name="precision" select="$finalPrecision"/>
		</xsl:call-template>
	</xsl:template>
	<!-- Legacy template kept for backwards compatibility -->
	<!-- Get currency symbol from currency code (ISO 4217 standard) -->
	<xsl:template name="getCurrencySymbol">
		<xsl:param name="currencyCode"/>
		<xsl:variable name="currency" select="exsl:node-set($currencySymbolsAndLabels)/currencies/currency[@code=$currencyCode]"/>
		<xsl:choose>
			<xsl:when test="$currency/symbol">
				<xsl:value-of select="$currency/symbol"/>
			</xsl:when>
			<xsl:otherwise>
				<!-- Comprehensive fallback for major world currencies -->
				<xsl:choose>
					<!-- Americas -->
					<xsl:when test="$currencyCode='USD'">$</xsl:when>
					<xsl:when test="$currencyCode='CAD'">$</xsl:when>
					<xsl:when test="$currencyCode='MXN'">$</xsl:when>
					<xsl:when test="$currencyCode='BRL'">R$</xsl:when>
					<xsl:when test="$currencyCode='ARS'">$</xsl:when>
					<xsl:when test="$currencyCode='CLP'">$</xsl:when>
					<xsl:when test="$currencyCode='COP'">$</xsl:when>
					<!-- Europe -->
					<xsl:when test="$currencyCode='EUR'">€</xsl:when>
					<xsl:when test="$currencyCode='GBP'">£</xsl:when>
					<xsl:when test="$currencyCode='CHF'">CHF</xsl:when>
					<xsl:when test="$currencyCode='SEK'">kr</xsl:when>
					<xsl:when test="$currencyCode='NOK'">kr</xsl:when>
					<xsl:when test="$currencyCode='DKK'">kr</xsl:when>
					<xsl:when test="$currencyCode='PLN'">zł</xsl:when>
					<xsl:when test="$currencyCode='CZK'">Kč</xsl:when>
					<xsl:when test="$currencyCode='HUF'">Ft</xsl:when>
					<xsl:when test="$currencyCode='RUB'">₽</xsl:when>
					<xsl:when test="$currencyCode='TRY'">₺</xsl:when>
					<xsl:when test="$currencyCode='RON'">lei</xsl:when>
					<!-- Asia-Pacific -->
					<xsl:when test="$currencyCode='JPY'">¥</xsl:when>
					<xsl:when test="$currencyCode='CNY'">¥</xsl:when>
					<xsl:when test="$currencyCode='KRW'">₩</xsl:when>
					<xsl:when test="$currencyCode='INR'">₹</xsl:when>
					<xsl:when test="$currencyCode='AUD'">$</xsl:when>
					<xsl:when test="$currencyCode='NZD'">$</xsl:when>
					<xsl:when test="$currencyCode='SGD'">$</xsl:when>
					<xsl:when test="$currencyCode='HKD'">$</xsl:when>
					<xsl:when test="$currencyCode='THB'">฿</xsl:when>
					<xsl:when test="$currencyCode='MYR'">RM</xsl:when>
					<xsl:when test="$currencyCode='IDR'">Rp</xsl:when>
					<xsl:when test="$currencyCode='PHP'">₱</xsl:when>
					<xsl:when test="$currencyCode='VND'">₫</xsl:when>
					<xsl:when test="$currencyCode='TWD'">NT$</xsl:when>
					<xsl:when test="$currencyCode='PKR'">₨</xsl:when>
					<xsl:when test="$currencyCode='BDT'">৳</xsl:when>
					<xsl:when test="$currencyCode='LKR'">Rs</xsl:when>
					<!-- Middle East & Africa -->
					<xsl:when test="$currencyCode='AED'">د.إ</xsl:when>
					<xsl:when test="$currencyCode='SAR'">﷼</xsl:when>
					<xsl:when test="$currencyCode='ILS'">₪</xsl:when>
					<xsl:when test="$currencyCode='ZAR'">R</xsl:when>
					<xsl:when test="$currencyCode='EGP'">£</xsl:when>
					<xsl:when test="$currencyCode='NGN'">₦</xsl:when>
					<xsl:when test="$currencyCode='KES'">KSh</xsl:when>
					<!-- Default: return currency code -->
					<xsl:otherwise>
						<xsl:value-of select="$currencyCode"/>
					</xsl:otherwise>
				</xsl:choose>
			</xsl:otherwise>
		</xsl:choose>
	</xsl:template>
	<!-- Get currency label from currency code -->
	<xsl:template name="getCurrencyLabel">
		<xsl:param name="currencyCode"/>
		<xsl:variable name="currency" select="exsl:node-set($currencySymbolsAndLabels)/currencies/currency[@code=$currencyCode]"/>
		<xsl:choose>
			<xsl:when test="$currency/label">
				<xsl:value-of select="$currency/label"/>
			</xsl:when>
			<xsl:otherwise>
				<!-- Comprehensive fallback for major world currencies -->
				<xsl:choose>
					<!-- Americas -->
					<xsl:when test="$currencyCode='USD'">US Dollar</xsl:when>
					<xsl:when test="$currencyCode='CAD'">Canadian Dollar</xsl:when>
					<xsl:when test="$currencyCode='MXN'">Mexican Peso</xsl:when>
					<xsl:when test="$currencyCode='BRL'">Brazilian Real</xsl:when>
					<xsl:when test="$currencyCode='ARS'">Argentine Peso</xsl:when>
					<xsl:when test="$currencyCode='CLP'">Chilean Peso</xsl:when>
					<xsl:when test="$currencyCode='COP'">Colombian Peso</xsl:when>
					<!-- Europe -->
					<xsl:when test="$currencyCode='EUR'">Euro</xsl:when>
					<xsl:when test="$currencyCode='GBP'">British Pound</xsl:when>
					<xsl:when test="$currencyCode='CHF'">Swiss Franc</xsl:when>
					<xsl:when test="$currencyCode='SEK'">Swedish Krona</xsl:when>
					<xsl:when test="$currencyCode='NOK'">Norwegian Krone</xsl:when>
					<xsl:when test="$currencyCode='DKK'">Danish Krone</xsl:when>
					<xsl:when test="$currencyCode='PLN'">Polish Zloty</xsl:when>
					<xsl:when test="$currencyCode='CZK'">Czech Koruna</xsl:when>
